DUPLICATE DATABASE TO DB11G
  FROM ACTIVE DATABASE
  SPFILE
  NOFILENAMECHECK;
  
run {
     ALLOCATE CHANNEL CH1 TYPE DISK;
     ALLOCATE AUXILIARY CHANNEL AUXCH1 TYPE DISK;
     SET NEWNAME FOR DATABASE TO 'C:\oracle\oradata\ORADB19S\%U'; 
     DUPLICATE TARGET DATABASE TO ORADB19S FROM ACTIVE DATABASE
     LOGFILE
       GROUP 1 ('C:\oracle\oradata\ORADB19S\REDO01.LOG') SIZE 50M, 
       GROUP 2 ('C:\oracle\oradata\ORADB19S\REDO02.LOG') SIZE 50M,
       GROUP 3 ('C:\oracle\oradata\ORADB19S\REDO03.LOG') SIZE 50M; 
}
